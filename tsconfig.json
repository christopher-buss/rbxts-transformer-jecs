{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "lib": [
      "ES2015",
      "DOM"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "eslint.config.ts",
    "test"
  ]
}